<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.0</TargetFramework>
    <UseWPF>true</UseWPF>
    <RootNamespace>Rail</RootNamespace>
    <AssemblyName>Rail</AssemblyName>
    <StartupObject>Rail.App</StartupObject>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Images\Help.png" />
    <None Remove="Images\Info.png" />
    <None Remove="Images\New.png" />
    <None Remove="Images\Open.png" />
    <None Remove="Images\Rail.png" />
    <None Remove="Images\Save.png" />
    <None Remove="Images\Settings.png" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Xaml.Behaviors.Wpf" Version="1.1.3" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Images\Help.png" />
    <Resource Include="Images\Info.png" />
    <Resource Include="Images\New.png" />
    <Resource Include="Images\Open.png" />
    <Resource Include="Images\Rail.png" />
    <Resource Include="Images\Save.png" />
    <Resource Include="Images\Settings.png" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Mvvm\DialogButtonsView.xaml.cs">
      <DependentUpon>DialogButtonsView.xaml</DependentUpon>
    </Compile>
    <Compile Update="Properties\Resources.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Resources.resx</DependentUpon>
    </Compile>
    <Compile Update="Properties\Settings.Designer.cs">
      <DesignTimeSharedInput>True</DesignTimeSharedInput>
      <AutoGen>True</AutoGen>
      <DependentUpon>Settings.settings</DependentUpon>
    </Compile>
    <Compile Update="View\CreateView.xaml.cs">
      <DependentUpon>CreateView.xaml</DependentUpon>
    </Compile>
    <Compile Update="View\MainView.xaml.cs">
      <SubType>Code</SubType>
      <DependentUpon>MainView.xaml</DependentUpon>
    </Compile>
    <Compile Update="View\RailPlanView.xaml.cs">
      <DependentUpon>RailPlanView.xaml</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Properties\Resources.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Resources.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

  <ItemGroup>
    <None Update="Properties\Settings.settings">
      <Generator>SettingsSingleFileGenerator</Generator>
      <LastGenOutput>Settings.Designer.cs</LastGenOutput>
    </None>
    <None Update="Tracks.xml">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>

  <ItemGroup>
    <Page Update="Mvvm\DialogButtonsView.xaml">
      <SubType>Designer</SubType>
    </Page>
    <Page Update="View\CreateView.xaml">
      <SubType>Designer</SubType>
    </Page>
    <Page Update="View\MainView.xaml">
      <SubType>Designer</SubType>
    </Page>
    <Page Update="View\RailPlanView.xaml">
      <SubType>Designer</SubType>
    </Page>
  </ItemGroup>

</Project>